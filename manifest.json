{
  "manifest_version": 3,
  "name": "Webpage to PDF Printer",
  "version": "1.0",
  "description": "Print any webpage to a high-quality, organized PDF.",
  "permissions": ["debugger", "activeTab", "downloads"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
